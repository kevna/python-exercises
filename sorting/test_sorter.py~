import unittest
import stupidSort
import insertionSort

class Test_Sorter(unittest.TestCase):
    TESTLIST = (
        ([6, 2, 4, 1, 3], [1, 2, 3, 4, 6]),
        ([-5, -1, -3, -10], [-10, -5, -3, -1]),
            )
    def test_StupidSort(self):
        testSorter = stupidSort.StupidSort()
        for testArgItems, expectedResult in self.TESTLIST:
            testSorter.sort(testArgItems)
            actualResult = testArgItems
            self.assertEqual(expectedResult, actualResult)

    def test_StupidSort(self):
        testSorter = insertionSort.InsertionSort()
        for testArgItems, expectedResult in self.TESTLIST:
            testSorter.sort(testArgItems)
            actualResult = testArgItems
            self.assertEqual(expectedResult, actualResult)

            
if __name__ == "__main__":
    unittest.main()
